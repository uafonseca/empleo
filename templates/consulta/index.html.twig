{% extends 'site/base.html.twig' %}
{% block body %}
    {{ include ('site/header.html.twig',{'notifications': notifications}  ) }}
    <div class="alice-bg padding-top-70 padding-bottom-70">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="breadcrumb-area">
                        <h1>Mis consulatas</h1>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item">
                                    <a href="{{ path('homepage') }}">Inicio</a>
                                </li>
                                <li class="breadcrumb-item active" aria-current="page">Mis consultas</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <a href="{{path('consulta_new')}}" class="btn btn-primary">Nueva consulta</a>
                </div>
            </div>
        </div>
    </div>
    <div class="alice-bg section-padding-bottom">
        <div class="container">
            <div class="row no-gutters">
                <div class="col-12">
                    {{ sg_datatables_render_html(datatable) }}
                </div>
            </div>
        </div>
    </div>

{% endblock %}

{% block scrips %}
    {{ sg_datatables_render_js(datatable) }}
{% endblock %}