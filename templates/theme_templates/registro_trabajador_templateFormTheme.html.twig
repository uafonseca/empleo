{% block _registro_trabajador_imagenFile_row %}
    <div class="col-md-12 form-group {% if (not compound or force_error|default(false)) and not valid %} has-error{% endif %}">
        {{- form_label(form) -}}
        {{- form_widget(form) -}}
        {{- form_errors(form) -}}
    </div>
{% endblock %}

{% block _registro_trabajador_dotacion_tallaCamisa_row -%}
    <div class="{{ attr_class|default('col-md-6') }} form-group {% if (not compound or force_error|default(false)) and not valid %} has-error{% endif %}">
        {{- form_label(form) -}}
        <span class="pull-right info-tallas" data-toggle="tooltip" data-placement="top"
              title="Este valor depende del género del trabajador" style="color: #0000ff;font-size: 15px"><i
                    class="fa fa-info-circle"></i></span>
        {{- form_widget(form) -}}
        {{- form_errors(form) -}}
    </div>
{%- endblock %}

