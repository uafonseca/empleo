{% extends 'backend/base.html.twig' %}

{% block title %}
{% endblock %}

{% block context %}
	<div class="card">
		<div class="card-header">Registro de creaci√≥n de usuarios</div>
		<div class="card-body">
			{{ form_start(form) }}
			<div class="row">
				<div class="col-md-6">
					{{ form_row(form.name) }}
				</div>
				<div class="col-md-6">
					{{ form_row(form.email) }}
				</div>
				<div class="col-md-6">
					{{ form_row(form.username) }}
				</div>
				<div class="col-md-6">
					{{ form_row(form.phone) }}
				</div>
				<div class="col-md-12 pt-5">
					<button type="submit" class="btn btn-primary"><i class="fa fa-save"></i>Guardar</button>
				</div>
			</div>
			{{ form_end(form) }}
		</div>
	</div>
{% endblock %}

{% block scripts %}

{% endblock %}
