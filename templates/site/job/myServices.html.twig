{% extends 'site/base.html.twig' %}
{% block title %}Mis servicios{% endblock %}
{% block body %}
    {{ include ('site/header.html.twig',{'notifications': notifications}  ) }}
    <div class="alice-bg padding-top-70 padding-bottom-70">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="breadcrumb-area">
                        <h1>Mis servicios</h1>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="{{ path('homepage') }}">Inicio</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Mis servicios</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="alice-bg section-padding-bottom">
        <div class="container">
            <div class="row no-gutters">
                <div class="col-12">
                    {{ sg_datatables_render_html(datatable) }}
                </div>
            </div>
        </div>
    </div>

    {% block js %}
        {{ sg_datatables_render_js(datatable) }}
        <script src="{{ asset('site/js/utils.js') }}"></script>
        <script src="{{ asset('site/js/ajax.js') }}"></script>
    {% endblock %}
{% endblock %}